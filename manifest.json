{
    "background": {
        "scripts": [ "js/jquery.min.js", "js/crypto_sha1.js", "js/lib.js", "js/bg.js" ]
    },
    "browser_action": {
        "default_icon": "img/icon_reload_19.png",
        "default_popup": "popup.html"
    },
    "options_page": "settings.html",
    "description": "Sends Requests in Background to keep session active",
    "icons": {
        "16": "img/16/icon_timed_reload_16.png",
        "48": "img/48/icon_timed_reload_48.png",
        "128": "img/128/icon_timed_reload_128.png"
    },

    "manifest_version": 2,
    "name": "Staying Alive",
    "permissions": [ "tabs", "activeTab", "background", "cookies", "webRequest", "storage", "webRequestBlocking", "\u003Call_urls\u003E" ],
    "version": "1.1"
}
