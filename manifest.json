{
    "background": {
        "scripts": [ "js/jquery.min.js", "js/crypto_sha1.js", "js/lib.js", "js/bg.js" ]
    },
    "browser_action": {
        "default_icon": "img/24/geek_zombie_24.png",
        "default_popup": "popup.html"
    },
    "options_page": "settings.html",
    "description": "Sends Requests in Background to keep session active",
    "icons": {
        "128": "img/128/geek_zombie_128.png",
        "16": "img/16/geek_zombie_16.png",
        "48": "img/48/geek_zombie_48.png"
    },

   /* "page_action": {
        "default_icon": {
            "19": "img/icon_reload_19.png"

        }
    },*/

    "manifest_version": 2,
    "name": "Staying Alive",
    "permissions": [ "tabs", "activeTab", "background", "cookies", "webRequest", "storage", "webRequestBlocking", "\u003Call_urls\u003E" ],
    "version": "1.0"
}
